import wollok.game.*
import nave.*

program prueba {
	game.title("prueba")
	//game.cellSize(5)
	game.height(20)
	game.width(20)
	game.addVisual(nave)
	keyboard.w().onPressDo({ nave.movimiento("arriba") })
	keyboard.s().onPressDo({ nave.movimiento("abajo") })
	keyboard.a().onPressDo({ nave.movimiento("izq") })
	keyboard.d().onPressDo({ nave.movimiento("der") })
	
	game.start()
}
